<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean wiring : telling spring what depend on what  -->
	<bean id="car" class="com.demo.Car" scope="prototype">
		<property name="brand" value="i10"/>
		<property name="tyre" ref="tyre"/>
	</bean>
	
	<bean id="tyre" class="com.demo.CeatTyre" scope="prototype"/>
</beans>
